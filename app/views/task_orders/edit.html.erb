        <div id="agreement-update-info">
          <%= render 'form' %>
        </div>
      </div>
      <!-- /.panel-body -->
    </div>
  </div>

  <div class="col-xs-6">
    <div class="panel panel-default">
      <div class="panel-heading">
        <i class="fa fa-file-text "></i> Current Information
      </div>
      <div class="panel-body">
        <div id="task-current-info">
          <%= render 'task_orders/current_content' %>
        </div>
      </div>
    </div>
  </div>
</div>